      "source": [
        "# Создаем три графика в одном столбце\n",
        "fig, axes = plt.subplots(nrows=3, ncols=1, figsize=(12, 15), sharex=True)\n",
        "\n",
        "\n",
        "palette = sns.color_palette(\"Set2\")\n",
        "\n",
        "# 1. График выручки\n",
        "sns.barplot(x='date', y='revenue', data=day_statistics, ax=axes[0], color=palette[0])\n",
        "axes[0].tick_params(axis='x', rotation=45)\n",
        "axes[0].set_title('Выручка', fontsize=14)\n",
        "axes[0].set_ylabel(\"Сумма, ₽\", fontsize=12)\n",
        "axes[0].grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# 2. График среднего чека\n",
        "sns.barplot(x='date', y='average_check', data=day_statistics, ax=axes[1], color=palette[1])\n",
        "axes[1].tick_params(axis='x', rotation=45)\n",
        "axes[1].set_title('Средний чек', fontsize=14)\n",
        "axes[1].set_ylabel(\"Сумма, ₽\", fontsize=12)\n",
        "axes[1].grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# 3. График количества заказов\n",
        "sns.barplot(x='date', y='count_orders', data=day_statistics, ax=axes[2], color=palette[2])\n",
        "axes[2].tick_params(axis='x', rotation=45)\n",
        "axes[2].set_title('Количество заказов', fontsize=14)\n",
        "axes[2].set_ylabel(\"Число заказов\", fontsize=12)\n",
        "axes[2].grid(axis='y', linestyle='--', alpha=0.7)\n",
        "\n",
        "# Улучшение читаемости оси X\n",
        "axes[2].set_xlabel(\"Дата\", fontsize=12)  # Устанавливаем подпись оси X только для последнего графика\n",
        "\n",
        "plt.tight_layout()  # Оптимизация размещения графиков\n",
        "plt.show()\n"
      ],
