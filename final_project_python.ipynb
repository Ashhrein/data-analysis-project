      "source": [
        "day_statistics = df_full.query('status == \"confirmed\"')\\\n",
        "                        .groupby(['date'], as_index=False)\\\n",
        "                        .agg(revenue = ('revenue', 'sum'),\n",
        "                             average_check = ('revenue', 'mean'),\n",
        "                             count_orders = ('order_id', 'count'))\n",
        "\n",
        "df_melted = day_statistics.melt(id_vars='date', value_vars=['revenue', 'average_check', 'count_orders'],\n",
        "                    var_name='metric', value_name='value')\n",
        "\n",
        "df_melted.head()"
      ],
      "id": "mTwJ6t1_8ylk"
    },
