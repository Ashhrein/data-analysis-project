        "canceled_day_befor = orders_status.query('date == \"2024-03-13\" and status == \"canceled\"')\\\n",
        "                                  .groupby(['order_id'], as_index=False)\\\n",
        "                                  .agg(count_products=('product_id', 'count'),\n",
        "                                       all_quantity=('quantity', 'sum'),\n",
        "                                       manager=('manager', 'max'),\n",
        "                                       client_id=('client_id', 'max'))\n",
        "\n",
        "\n",
        "confirmed_on_day = orders_status.query('date == \"2024-03-14\" and status == \"confirmed\"')\\\n",
        "                                .groupby(['order_id'], as_index=False)\\\n",
        "                                .agg(count_products=('product_id', 'count'),\n",
        "                                       all_quantity=('quantity', 'sum'),\n",
        "                                       manager=('manager', 'max'),\n",
        "                                       client_id=('client_id', 'max'))\n",
        "\n",
        "reregistered_orders = pd.merge(canceled_day_befor, confirmed_on_day, on =['manager', 'client_id', 'count_products', 'all_quantity'])\n",
        "\n",
        "reregistered_orders"
