      "source": [
        "total_revenue = df_full.query('status == \"confirmed\"').revenue.sum()\n",
        "total_orders = df_full.query('status == \"confirmed\"').order_id.nunique()\n",
        "\n",
        "manager_statistics = df_full.query('status == \"confirmed\"')\\\n",
        "                            .groupby('manager', as_index=False)\\\n",
        "                            .agg(count_orders=('order_id', 'nunique'), revenue=('revenue', 'sum'))\n",
        "\n",
        "manager_statistics['share_of _total_revenue'] = round(manager_statistics.revenue/total_revenue, 3) * 100\n",
        "manager_statistics['share_of _total_orders'] = round(manager_statistics.count_orders/total_orders, 3) * 100\n",
        "\n",
        "\n",
        "\n",
        "manager_statistics.sort_values(['share_of _total_revenue', 'share_of _total_orders'], ascending=False)"
      ],
